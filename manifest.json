{
    "manifest_version": 2,

    "name": "MifiMonitor",
    "description": "Useful Stats for your Mifi",
    "version": "1.0",

    "permissions": [
        "http://mifi.admin/",
        "alarms"
    ],
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "icons": {
        "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
    //icons from http://www.iconarchive.com/show/windows-8-metro-icons-by-dakirby309/Other-Signal-Metro-icon.html
}